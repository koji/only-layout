{
  "name": "shintarou",
  "private": false,
  "version": "0.0.35",
  "type": "module",
  "description": "shintarou/新太郎 is a react component that helps to create a layout easily. It supports normal div, flexbox and grid.",
  "scripts": {
    "build": "pnpm run clean && tsc && vite build",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "cz": "git-cz",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -o docs-build",
    "test": "vitest",
    "test-run": "vitest run",
    "coverage": "vitest run --coverage",
    "clean": "shx rm -rf dist types"
  },
  "engines": {
    "node": ">=18.16.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/styled-components": "^5.1.32",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^0.34.6",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.54.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^5.11.1",
    "git-cz": "^4.9.0",
    "jsdom": "^22.1.0",
    "json": "^11.0.0",
    "prettier": "^2.8.8",
    "react-hooks": "^1.0.1",
    "shx": "^0.3.4",
    "storybook": "6.5.16",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-compression2": "^0.10.6",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.34.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.0",
    "react": "^16.8.0 || 18.x",
    "react-dom": "^16.8.0 || 18.x",
    "styled-components": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/shintarou.umd.js",
  "module": "./dist/shintarou.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/shintarou.es.js",
      "require": "./dist/shintarou.umd.js"
    }
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "flexbox",
    "grid",
    "layout"
  ],
  "author": {
    "name": "koji kanao"
  },
  "bugs": {
    "url": "https://github.com/koji/shintarou/issues"
  },
  "homepage": "https://github.com/koji/shintarou",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koji/shintarou"
  }
}
