{
  "name": "shintarou",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "cz": "git-cz",
    "storybook": "start-storybook -p 6006",
    "test": "vitest",
    "testrun": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.34.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "json": "^11.0.0",
    "prettier": "^2.8.4",
    "react-hooks": "^1.0.1",
    "storybook": "6.5.16",
    "typescript": "^5.0.4",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.2",
    "vitest": "^0.31.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.0",
    "react": "^16.8.0 || 18.x",
    "react-dom": "^16.8.0 || 18.x",
    "styled-components": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/shintarou.umd.js",
  "module": "./dist/shintarou.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/shintarou.es.js",
      "require": "./dist/shintarou.umd.js"
    }
  },
  "keywords": [
    "react",
    "layout"
  ],
  "author": "koji",
  "bugs": {
    "url": "https://github.com/koji/shin/issues"
  },
  "homepage": "https://github.com/koji/only-layout#readme",
  "license": "MIT"
}
